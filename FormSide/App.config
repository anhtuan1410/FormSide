<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>
  <entityFramework>
    <providers>
      <provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer" />
    </providers>
  </entityFramework>
  <connectionStrings>
    <add name="SampleDatabase111" providerName="System.Data.ProviderName"
     connectionString="Server=.\SQLEXPRESS;Database=SampleDatabase;User Id=sa;Password=sa2222;" />
    <add name="SampleDatabase222" providerName="System.Data.ProviderName"
     connectionString="Server=.\SQLEXPRESS;Database=SampleDatabase;Trusted_Connection=True;" />
    <add name="SampleDatabase"
             connectionString="Data Source=.\SQLEXPRESS;Initial Catalog=SampleDatabase;Integrated Security=true"
             providerName="System.Data.SqlClient"/>
  </connectionStrings>
</configuration>